<div class="words-view">
    <% i = 1 %>
    <%= link_to "#", id: "li1", class: "code", target: "_blank" do%>
      <span>01</span>
    <% end %>
    <% while 1 <= i && i <= 8 %>
      <% i += 1 %>
      <%= link_to "#", id: "li" + i.to_s, target: "_blank" do%>
      <span>0<%= i %></span>
    <% end %>
    <% end %>
    <% while 9 <= i && i <= 29 %> 
      <% i += 1 %>
      <%= link_to "#", id: "li" + i.to_s, target: "_blank" do%>
      <span><%= i %></span>
    <% end %>
    <% end %>
</div>