<div class="words-view">
  <ul>
    <% i = 1 %>
    <li class="li1" id="code"><span>01</span></li>
    <% while 1 <= i && i <= 8 %>
      <% i += 1 %>
      <li class="li<%= i %>"><span>0<%= i %></span></li>
    <% end %>
    <% while 9 <= i && i <= 29 %> 
      <% i += 1 %>
      <li class="li<%= i %>"><span><%= i %></span></li>
    <% end %>
  </ul>
</div>