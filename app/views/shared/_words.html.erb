<div class="words-view">
  <ul>
    <% i = 1 %>
    <p >01</p>
    <li class="li1" id="code"></li>
  </ul>
    <% while 1 <= i && i <= 8 %>
      <ul>
        <% i += 1 %>
        <p>0<%= i %></p>
        <li class="li<%= i %>"></li>
      </ul>
    <% end %>
    <% while 9 <= i && i <= 29 %> 
      <ul>
        <% i += 1 %>
        <p><%= i %></p>
        <li class="li<%= i %>"></li>
      </ul>
    <% end %>
</div>