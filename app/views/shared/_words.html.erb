<div class="words-view">
  <ul>
    <% i = 1 %>
    <li class="li1" id="code">01: </li>
    <% while 1 <= i && i <= 8 %> 
      <% i += 1 %>
      <li class="li<%= i %>">0<%= i %>: </li>
    <% end %>
    <% while 9 <= i && i <= 29 %> 
      <% i += 1 %>
      <li class="li<%= i %>"><%= i %>: </li>
    <% end %>
  </ul>
</div>